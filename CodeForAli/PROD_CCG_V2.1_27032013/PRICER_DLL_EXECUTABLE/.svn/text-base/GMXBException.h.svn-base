#ifndef __GMXB_EXCEPTION_H_
#define __GMXB_EXCEPTION_H_

class GMXBException
{
	private:
		char * message;

	public:
		~GMXBException()
		{

		}

		GMXBException(int code_error, char* msg)
		{
			
		}

		GMXBException(char* msg)
		{
			
			this->message = msg;
			
		}

		char * get_message()
		{
			return this->message;
		
		}


};

#endif